{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": ["./Workinput","./ScriptLibrary"],
        "exclude": [],
        "includePattern": "(.+\\.js?$)",
        "excludePattern": "(jquery)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true
    },
    "opts": {
        "template": "./node_modules/docdash/",
        "encoding": "utf8",
        "destination": "./doc/",
        "recurse": true,
        "tutorials": "./doc_md"
    },
    "docdash": {
        "sectionOrder": ["Tutorials","Classes","Globals"],
        "collapse": true,
        "search": true
    }
}

