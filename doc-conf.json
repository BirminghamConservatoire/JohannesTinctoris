{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": ["./editor/music","./ScriptLibrary", "./Tinctoris/Music"],
        "exclude": [],
        "includePattern": "(.+\\.js?$)",
        "excludePattern": "(jquery)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "./node_modules/docdash/",
        "encoding": "utf8",
        "destination": "./doc/",
        "recurse": true,
        "tutorials": "./doc_md",
        "readme": "./Tinctoris/readme.md"
    },
    "docdash": {
        "sectionOrder": ["Tutorials","Modules", "Namespaces","Classes","Globals"],
        "collapse": true,
        "search": true,
        "scopeInOutputPath": false,
        "nameInOutputPath": false,
        "versionInOutputPath": false
    }
}

